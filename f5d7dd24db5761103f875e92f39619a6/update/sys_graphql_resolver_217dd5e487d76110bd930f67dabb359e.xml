<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_graphql_resolver">
    <sys_graphql_resolver action="INSERT_OR_UPDATE">
        <name>Record Resolver</name>
        <schema display_value="Get ESRI Map Location Records">6268d92087d76110bd930f67dabb350e</schema>
        <script><![CDATA[(function process(/*ResolverEnvironment*/ env) {

	var records = [];
    var gr = new GlideRecord(env.getArguments().tableName);
	gr.addEncodedQuery(env.getArguments().encodedQuery);
	gr.query();
	
	while(gr.next()) {
		records.push({
			sys_id: gr.getUniqueValue(),
			tableName: env.getArguments().tableName
		});
		
	}

	return records;
	
})(env);]]></script>
